@article{khan_deposition_2008,
	Abstract = {Lead titanate {(PbTiO3)} thin films were prepared on platinised silicon substrates by pulsed laser deposition. A predominantly (1\&\#xa0;1\&\#xa0;1) oriented perovskite phase was obtained at a substrate temperature of {530\&\#xa0;$\,^{\circ}$C.} Deposition at higher substrate temperatures resulted in the formation of a lead deficient pyrochlore phase {PbTi3O7.} The (1\&\#xa0;1\&\#xa0;1) orientation was slightly enhanced with an increase in the oxygen deposition pressure from {100\&\#xa0;mTorr} to {150\&\#xa0;mTorr.} A similar effect was observed with an increase in the post-deposition annealing temperature from {400\&\#xa0;$\,^{\circ}$C} to {600\&\#xa0;$\,^{\circ}$C.} {P--E} loops with remanent polarization {(Pr)} as high as {40\&\#xa0;μC/cm2} and coercive field {Ec\&\#xa0;=\&\#xa0;105\&\#xa0;kV/cm} were obtained under an electric field of an amplitude of {270\&\#xa0;kV/cm} at {50\&\#xa0;kHz.} Impedance spectroscopy and leakage current behaviour of the films are also reported.},
	Author = {Khan, Mikael A. and Comyn, Timothy P. and Bell, Andrew J.},
	Date-Added = {2012-08-10 16:42:29 +0000},
	Date-Modified = {2012-08-10 16:42:29 +0000},
	Doi = {10.1016/j.jeurceramsoc.2007.07.023},
	File = {ScienceDirect Full Text PDF:files/312/Khan et al. - 2008 - Deposition of PbTiO3 films on PtSi substrates usi.pdf:application/pdf;ScienceDirect Snapshot:files/313/S0955221907004487.html:text/html},
	Issn = {0955-2219},
	Journal = {Journal of the European Ceramic Society},
	Keywords = {Electron microscopy, Ferroelectric properties, Films, {PbTiO3}, Perovskites, Pulsed laser deposition {(PLD)}},
	Number = {3},
	Pages = {591--597},
	Title = {Deposition of {PbTiO3} films on {Pt/Si} substrates using pulsed laser deposition},
	Url = {http://www.sciencedirect.com/science/article/pii/S0955221907004487},
	Volume = {28},
	Year = {2008},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0955221907004487},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.jeurceramsoc.2007.07.023}}

@article{morales_nanostructured_2004,
	Abstract = {Nanostructured obtained by hydrolysis of is shown to exhibit an excellent electrochemical activity as a thin electrode in lead-acid cells. This material was characterized by X-ray diffraction, electron microscopy, X-ray photoelectron spectroscopy, thermogravimetric analysis, and Brunauer-Teller-Emmett measurements. The electrode was prepared by spray deposition of the nanostructured material and showed high utilization (about 65\% of the theoretical capacity) and good cycling stability at discharge current of 1 C. It is suggested that the nanostructure is the key factor for the enhanced electrochemical activity of lead dioxide. {\copyright} 2004 The Electrochemical Society. All rights reserved.},
