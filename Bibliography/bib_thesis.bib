	Issn = {00218979},
	Journal = {Journal of Applied Physics},
	Month = aug,
	Number = {3},
	Pages = {1920--1925},
	Title = {Raman spectroscopy and X‐ray diffraction of {PbTiO{$_{3}$}} thin film},
