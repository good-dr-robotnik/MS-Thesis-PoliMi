	Doi = {doi:10.1063/1.360229},
	File = {AIP Journal PDF:files/143/Ching‐Prado et al. - 1995 - Raman spectroscopy and x‐ray diffraction of PbTiO3.pdf:application/pdf;AIP Journal Snapshot:files/145/p1920_s1.html:text/html},
	Issn = {00218979},
	Journal = {Journal of Applied Physics},
	Month = aug,
	Number = {3},
	Pages = {1920--1925},
	Title = {Raman spectroscopy and X‐ray diffraction of {PbTiO{$_{3}$}} thin film},
	Url = {http://jap.aip.org/resource/1/japiau/v78/i3/p1920_s1},
