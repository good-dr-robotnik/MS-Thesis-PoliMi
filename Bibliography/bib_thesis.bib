@article{robertson_shallow_1993,
	Abstract = {A combination of electron paramagnetic resonance and band structure calculations is used to show that the Pb2+ ions in lead zirconate titanate materials can act as shallow hole traps to give Pb3+ centers. The Pb3+ centers are created by optical illumination and are found to be metastable at room temperature. Their decay kinetics follow a stretched exponential function which suggests dispersive transport. The traps have the same character as the upper valence band which is formed from the hybridization of Pb s and O p states.},
	Author = {Robertson, John and Warren, William L. and Tuttle, Bruce A. and Dimos, Duane and Smyth, Donald M.},
	Date-Added = {2012-08-10 12:49:29 +0000},
	Date-Modified = {2012-08-10 12:49:29 +0000},
	Doi = {doi:10.1063/1.110761},
	File = {AIP Journal PDF:files/135/Robertson et al. - 1993 - Shallow Pb3+ hole traps in lead zirconate titanate.pdf:application/pdf;AIP Journal Snapshot:files/137/p1519_s1.html:text/html},
	Issn = {00036951},
	Journal = {Applied Physics Letters},
	Month = sep,
	Number = {11},
	Pages = {1519--1521},
	Title = {Shallow Pb3+ hole traps in lead zirconate titanate ferroelectrics},
	Url = {http://apl.aip.org/resource/1/applab/v63/i11/p1519_s1},
	Volume = {63},
	Year = {1993},
	Bdsk-Url-1 = {http://apl.aip.org/resource/1/applab/v63/i11/p1519_s1},
	Bdsk-Url-2 = {http://dx.doi.org/10.1063/1.110761},
	Bdsk-File-1 = {}}

@article{robertson_band_1995,
	Abstract = {Band structure calculations and electron paramagnetic resonance measurements are used to show that Pb states determine many of the electronic properties of {Pb(Zr,Ti)O3} ferroelectric materials. The valence‐band edge consists of hybridized Pb s and O p states at all compositions. The conduction‐band minimum changes from a Ti d‐like Γ25′ state to a Pb p‐like X1 state with increasing Zr content. The Pb p character accounts for the relatively small 0.2 {eV} increase in band gap in the {Pb(Ti,Zr)O3} alloys with Zr content compared to the large 2 {eV} increase in band gap in {Ba(Ti,Zr)O3} alloys. The paramagnetic Pb3+ hole center is found to become deeper and acquire some p character as the Zr content is raised. This is attributed to the change in conduction‐band character combined with a local off‐center displacement of the Pb3+ ion. {\copyright} 1995 American Institute of Physics.},
	Author = {Robertson, J. and Warren, W. L. and Tuttle, B. A.},
	Date-Added = {2012-08-10 11:14:02 +0000},
	Date-Modified = {2012-08-10 11:14:02 +0000},
	Doi = {doi:10.1063/1.358580},
	File = {AIP Journal PDF:files/110/Robertson et al. - 1995 - Band states and shallow hole traps in Pb(Zr,Ti)O3 .pdf:application/pdf;AIP Journal Snapshot:files/111/p3975_s1.html:text/html},
	Issn = {00218979},
	Journal = {Journal of Applied Physics},
	Month = apr,
	Number = {8},
	Pages = {3975--3980},
	Title = {Band states and shallow hole traps in {Pb(Zr,Ti)O3} ferroelectrics},
	Url = {http://jap.aip.org/resource/1/japiau/v77/i8/p3975_s1},
	Volume = {77},
	Year = {1995},
	Bdsk-Url-1 = {http://jap.aip.org/resource/1/japiau/v77/i8/p3975_s1},
