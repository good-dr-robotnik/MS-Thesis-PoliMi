	File = {ScienceDirect Full Text PDF:files/595/Cianci et al. - 2012 - Atomic layer deposited TiO2 for implantable brain-.pdf:application/pdf;ScienceDirect Snapshot:files/596/S0040609011019389.html:text/html},
	Issn = {0040-6090},
	Journal = {Thin Solid Films},
	Keywords = {{ALD} {TiO2}, Biocompatibility, High k, Neurochip},
