	Issn = {0254-0584},
	Journal = {Materials Chemistry and Physics},
	Keywords = {Anatase titanium dioxide, Bandgap, Blue shift, nanoparticles, Optical properties},
	Month = feb,
	Number = {1},
	Pages = {239--245},
	Title = {Bandgap studies on anatase titanium dioxide nanoparticles},
