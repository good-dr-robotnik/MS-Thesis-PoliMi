	Journal = {Journal of Vacuum Science \& Technology A: Vacuum, Surfaces, and Films},
	Keywords = {atomic layer deposition, desorption, high-k dielectric thin films, surface chemistry, titanium compounds},
	Number = {1},
	Pages = {01A150},
	Title = {Role of surface intermediates in enhanced, uniform growth rates of {TiO{\_{2}}} atomic layer deposition thin films using titanium tetraisopropoxide and ozone},
