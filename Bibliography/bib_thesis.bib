	Issn = {00036951},
	Journal = {Applied Physics Letters},
	Month = apr,
	Number = {14},
	Pages = {2059--2061},
	Title = {A monoclinic ferroelectric phase in the {Pb(Zr1−xTix)O3} solid solution},
	Url = {http://apl.aip.org/resource/1/applab/v74/i14/p2059_s1},
