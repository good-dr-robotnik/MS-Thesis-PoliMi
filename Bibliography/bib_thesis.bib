	Issn = {00036951},
	Journal = {Applied Physics Letters},
	Month = may,
	Number = {19},
	Pages = {2421--2423},
	Title = {Crystal orientation dependence of piezoelectric properties of lead zirconate titanate near the morphotropic phase boundary},
	Url = {http://apl.aip.org/resource/1/applab/v72/i19/p2421_s1},
