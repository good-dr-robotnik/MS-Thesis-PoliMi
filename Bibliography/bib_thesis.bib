	Journal = {Japanese Journal of Applied Physics},
	Pages = {031102},
	Title = {{{{\textbackslash}emph{In-situ}}} X-ray Photoemission Spectroscopy Study of Atomic Layer Deposition of {TiO{\_{2}}} on Silicon Substrate},
	Url = {http://jjap.jsap.jp/link?JJAP/51/031102/},
