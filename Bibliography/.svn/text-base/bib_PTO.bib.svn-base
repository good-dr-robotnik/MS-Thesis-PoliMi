@article{khan_deposition_2008,
	Abstract = {{{\textless}p{\textgreater}{\textless}br/{\textgreater}Lead} titanate {(PbTiO3)} thin films were prepared on platinised silicon substrates by pulsed laser deposition. A predominantly (1 1 1) oriented perovskite phase was obtained at a substrate temperature of 530 {$\,^{\circ}$C.} Deposition at higher substrate temperatures resulted in the formation of a lead deficient pyrochlore phase {PbTi3O7.} The (1 1 1) orientation was slightly enhanced with an increase in the oxygen deposition pressure from 100 {mTorr} to 150 {mTorr.} A similar effect was observed with an increase in the post-deposition annealing temperature from 400 {$\,^{\circ}$C} to 600 {$\,^{\circ}$C.} {P-E} loops with remanent polarization {(Pr)} as high as 40 {[mu]C/cm2} and coercive field Ec = 105 {kV/cm} were obtained under an electric field of an amplitude of 270 {kV/cm} at 50 {kHz.} Impedance spectroscopy and leakage current behaviour of the films are also reported.{\textless}/p{\textgreater}},
	Annote = {Useful for {XRD} spectra of perovskite {PbTiO3}},
	Author = {Khan, Mikael A. and Comyn, Timothy P. and Bell, Andrew J.},
	Doi = {16/j.jeurceramsoc.2007.07.023},
	File = {ScienceDirect Full Text PDF:files/68/Khan et al. - 2008 - Deposition of PbTiO3 films on PtSi substrates usi.pdf:application/pdf;ScienceDirect Snapshot:files/69/S0955221907004487.html:text/html},
	Issn = {0955-2219},
	Journal = {Journal of the European Ceramic Society},
	Keywords = {Electron microscopy, Ferroelectric properties, Films, {PbTiO3}, Perovskites, Pulsed laser deposition {(PLD)}},
	Number = {3},
	Pages = {591--597},
	Title = {Deposition of {PbTiO3} films on {Pt/Si} substrates using pulsed laser deposition},
	Url = {http://www.sciencedirect.com/science/article/pii/S0955221907004487},
	Volume = {28},
	Year = {2008},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0955221907004487},
	Bdsk-Url-2 = {http://dx.doi.org/16/j.jeurceramsoc.2007.07.023}}

